                        <input id="custEntMainId" class="form-control" type="text" maxlength="80">
                    </div><p></p>
                </div>
                <div class="form-group cform col-md-12">
                    <b class="col-md-2"> Line 1 </b>
                    <div class="col-md-10">
                        <input id="create-Address-1" class="form-control" type="text" maxlength="80">
                    </div><p></p>
                </div>
                <div class="form-group cform col-md-12">
                    <b class="col-md-2"> Line 2 </b>
                    <div class="col-md-10">
