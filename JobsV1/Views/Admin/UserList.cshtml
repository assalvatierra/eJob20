@model IEnumerable<JobsV1.Models.AppUser>

@{
    ViewBag.Title = "UserList";
    Layout = "~/Views/Shared/_ModuleLayout.cshtml";
}

<h2>Agent Assignment </h2>
<p>
    By User  |
    @Html.ActionLink("By Company", "CompanyList")
</p>
 
<table class="table">
    <tr>
        <th>
            User
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                <a id="catbtn" data-toggle="modal" data-target="#UserCompanyList" class="cursor-hand"
                   style="margin-bottom:10px;" onclick="UserCompanyList('@item.UserName')">Assigned</a>
            </td>
        </tr>
    }

</table>

    @{Html.RenderPartial("UserListCompanies", null, new ViewDataDictionary { { "CompanyList", ViewBag.CompanyList } });}

@Html.ActionLink("Back to List", "Index", null, new { @class = "control-text" }) 

<script src="~/Scripts/jquery-3.3.1.js"> </script>
<script src="~/Scripts/Admin/UserList.js"></script>
