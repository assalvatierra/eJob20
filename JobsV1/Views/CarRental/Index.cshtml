<!-- Car Selection -->
<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-md-4 car-primary">
            <h2>@Html.DisplayFor(modelItem => item.Description)</h2>
            <img src="~/CarRental/MainImage/@Html.DisplayFor(modelItem => item.Id)" class="carrental-car-img" alt="@item.Description" title="@item.Description" >
                <p>&nbsp</p>
            <div>
                <table class="table-no-border">
                    <tr>
                        <th>
                            @Html.DisplayFor(modelItem => item.Remarks)  &nbsp;
                        </th>
                    </tr>
                    <tr>
                        <td>
                            @Html.Action("CarRate", new { unitid = int.Parse(@Html.DisplayFor(modelItem => item.Id).ToString()) })
                        </td>
                    </tr>
                </table>
            </div>
        </div>
    }
</div>

