@section Scripts{
    <script type="text/javascript" src="~/Scripts/Modal.js"></script>
    <script>
        $(() => {

            setTimeout(function () {
               
                $('html,body').animate({
                    scrollTop: $(window).scrollTop() - 50
                });

                var link = $(location).attr('hash');

                $(link).children('h2').addClass('text-primary');
            }, 300);


        });
       
    </script>
}