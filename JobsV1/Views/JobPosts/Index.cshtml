@model IEnumerable<JobsV1.Models.JobPost>

@{
    ViewBag.Title = "Index";
}

<h2>Job Posting</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DtPost)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PaymentAmt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExpensesAmt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CarRentalInc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TourInc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OthersInc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Remarks)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.JobMain.Description)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DtPost)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PaymentAmt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ExpensesAmt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CarRentalInc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TourInc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OthersInc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Remarks)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JobMain.Description)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
