@model IEnumerable<JobsV1.Models.SupplierPoDtl>
@{ int SvcId = Convert.ToInt32(ViewData["svcId1"]);  }

<div>
    <span>Supplier Items </span>
    <span>&#187;</span>
    <div style="padding-left:1.5em;font-size:12px;">
        @foreach (var poTmp in Model)
        {
            <div>
                 <b id='@poTmp.Id' data_svcid="@SvcId" name="@poTmp.Id"  >
                        @poTmp.SupplierPoHdr.Supplier.Name 
                  </b>

                @foreach (var item in poTmp.SupplierPoItems)
                {
                <span>
                    [<b id="@item.Id" data_svcid="@SvcId" name="@item.Id" class="clickitem">
                        @item.InvItem.ItemCode  @item.InvItem.Description
                    </b>
                    ] &nbsp;
                </span>

                }
            </div>
        }

    </div>
</div>



